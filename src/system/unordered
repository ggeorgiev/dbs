#pragma once

#include <shared_ptr>
#include <unordered_set> // IWYU pragma: export
#include <__hash_table>  // IWYU pragma: export

template <typename T>
struct unordered_set : public enable_make_shared<unordered_set<T>>, 
                       public std::unordered_set<T>
{
    using std::unordered_set<T>::unordered_set;
};